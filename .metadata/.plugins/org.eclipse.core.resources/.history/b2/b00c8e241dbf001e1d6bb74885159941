package com.sanket;

public class StarPrint {

	public static void main(String[] args) {
		int n=5;
		for(int i=1;i<4;i++)
		{
			for(int j=0;j<=n;j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		System.out.println("-----------");
		
		
		for(int i=1;i<=4;i++)
		{
			for(int j=0;j<=6;j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		
		
		
		
		
		
		
		
		
		
		
	}

}

