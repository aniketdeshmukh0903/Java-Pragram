package com.sanket;

public class Star3 {
	public static void main(String[] args) 
		
	{
			for(int i=1;i<=4;i++)
			{
				for(int j=3;j>=i;j--)
				{
					System.out.println(" ");
				}
				System.out.print("*");
			}
			for(int l=1;l<=4;l++) 
			{
				for(int k=1;k<=l;k++)
				{
						System.out.print("*");
					}
					System.out.println();
				}

}
}
